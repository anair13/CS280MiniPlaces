net: "./vgg/train_baseline.prototxt"
test_state: { 
    stage: 'test-on-val'
}
test_iter: 10
test_interval: 5000
base_lr: 0.01
display: 10
max_iter: 50000
lr_policy: "step"
gamma: 0.1
momentum: 0.9
weight_decay: 0.0005
stepsize: 10000
snapshot: 5000
snapshot_prefix: "./snapshot/vgg_net"
random_seed: 1
average_loss: 10
iter_size: 1
type: "SGD"
